{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "useDefineForClassFields": false,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true
  },
  "include": ["typechain"]
}
